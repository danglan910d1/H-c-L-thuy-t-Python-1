    "def longestword (L) :\n",
    "    longest = ''\n",
    "    for i in L :\n",
    "        if len(i) > len(longest) :\n",
    "            longest = i\n",
    "    print(longest)\n",
    "L = ['Trần', 'Thị', 'Thu', 'Hà']\n",
    "longestword(L)\n"
