    "BUFFERSIZE = 250\n",
    "rFile = open(\"data/text.txt\", \"r\")\n",
    "wFile = open(\"data/Write.txt\", \"w\")\n",
    "buffer = rFile.read(BUFFERSIZE)\n",
    "\n",
    "i = 0\n",
    "while (len(buffer)) :\n",
    "    i = i + 1\n",
    "    wFile.write(buffer)\n",
    "    print(i, \"{} bye\".format(len(buffer)))\n",
    "    buffer = rFile.read(BUFFERSIZE)\n",
    "print(\"done\")\n",
    "\n",
    "\n"
